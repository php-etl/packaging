{
    "name": "php-etl/packaging",
    "description": "This library aims at building TAR archives, using PHP resources and streams",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "nikic/php-parser": "^4.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "johnkary/phpunit-speedtrap": "*",
        "mybuilder/phpunit-accelerator": "*",
        "phpunit/php-invoker": "*"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Packaging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "unit\\Kiboko\\Component\\Packaging\\": "tests/unit/",
            "functional\\Kiboko\\Component\\Packaging\\": "tests/functional/",
            "integration\\Kiboko\\Component\\Packaging\\": "tests/integration/",
            "performance\\Kiboko\\Component\\Packaging\\": "tests/performance/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
